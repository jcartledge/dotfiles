[user]
  email = jcartledge@gmail.com
  name = James Cartledge
[color]
  branch = auto
  diff = auto
  status = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[alias]
  s = status -s
[core]
  editor = /usr/bin/vim
	excludesfile = /Users/jcartledge/.gitignore_global
[merge]
  tool = mvim -d

[push]
  default = current

[pull]
  default = current
[init]
	templatedir = /Users/jcartledge/.git-template
	defaultBranch = main
[url "https://"]
	insteadOf = git://
