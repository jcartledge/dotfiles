{
  "coc.preferences.colorSupport": true,
  "coc.preferences.formatOnSaveFiletypes": ["javascript", "json", "css", "typescript"],
  "coc.preferences.formatOnType": false,
  "coc.preferences.jumpCommand": "edit",
  "diagnostic.enable": true,
  "diagnostic.errorSign": "X",
  "diagnostic.hintSign": "ⓘ",
  "diagnostic.infoSign": "ⓘ",
  "diagnostic.signOffset": 1000,
  "diagnostic.warningSign": "!",
  "snippets.priority": 99,
  "suggest.autoTrigger": "always",
  "suggest.noselect": false,
  "suggest.preferCompleteThanJumpPlaceholder": true,
  "suggest.timeout": 500,
  "suggest.triggerAfterInsertEnter": true
}
